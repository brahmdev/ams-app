{
  "expo": {
    "name": "Academy Management App",
    "slug": "ams-app",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "./assets/animations/*",
      "./assets/images/*",
      "./assets/fonts/*",
      "./assets/icons/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ams.rs",
      "versionCode": 2
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "native-base-shoutem-theme"
          ]
        }
      }
    },
    "description": "Academy Management App"
  }
}